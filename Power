import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int num =1010;
	   
	    int value=0;
	    int c=0;
	while(num!=0)
	{
	    
	    if(num%2==0)
	    {
	        
	        num=num/10;
	    }else{
	        value+= Math.pow(2,c);
	        c++;
	    }
	    
	}
	System.out.println(value);
	}
}

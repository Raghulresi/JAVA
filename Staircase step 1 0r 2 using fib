import java.util.*;
class Main{
    public static int fib(int n)
    {
        
        int b=2;
        int a =1;
        int c =0;
        for(int i=0;i<n-2;i++){
           
           c=a+b;
            a=b;
            b=c;
        }
         
        return c;
    }
    
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int result=fib(num);
        System.out.println(result);
    }
}


import java.util.*;
public class Main
{
	public static void main(String[] args) throws Exception {
		Scanner sc = new Scanner(System.in);
		String s = sc.nextLine();
		  

		String[] arr = s.split(" ");
		int n = arr.length;
		for(String a : arr)
		{  
		    for(int i=a.length()-1;i>=0;i--)
		    {   System.out.print(a.charAt(i)+"");
		    }
		    
		    System.out.print(" ");
		}
		
		
	}
}
